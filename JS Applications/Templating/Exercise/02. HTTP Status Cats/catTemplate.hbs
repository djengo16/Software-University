<li>
    <img src="images/{{this.imageLocation}}.jpg" width="250" height="250" alt="Card img cap">
    <div class="info">
        <button class="showBtn">Show status code btn</button>
        <div class="status" style="display: none;" id="{{this.id}}">
            <h4>Status Code: {{this.statusCode}}</h4>
            <p>{{this.statusMessage}}</p>
        </div>
    </div>
</li>